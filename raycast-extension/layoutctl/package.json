{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "layoutctl",
  "title": "Layout Control",
  "description": "Browse and restore layoutctl profiles.",
  "icon": "icon.png",
  "author": "Joba",
  "categories": ["Developer Tools"],
  "license": "MIT",
  "commands": [
    {
      "name": "restore-layout",
      "title": "Restore Layout",
      "description": "Pick a saved layoutctl profile",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix"
  }
}

